{
  "name": "ej-angular",
  "version": "16.3.0.21",
  "scripts": {
    "start": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --progress --port 8080 --open",
    "build": "rimraf dist && node --max_old_space_size=8192 .\\node_modules\\webpack\\bin\\webpack.js --config config/webpack.prod.js --progress",
    "lint": "tslint \"**/*.ts\" -c tslint.json -e \"typings/**\" -e \"node_modules/**\" -e \"app/ej/**\" -e \"app/services/**\" -e \"tmp/**\" -e \"deps/**\"",
    "copy-ejDef": "copy \"..\\..\\typescript\\ej.web.all.d.ts\" \"node_modules\\@types\\ej.web.all\\index.d.ts\" /y",
    "copy-components": "xcopy app\\components dist\\app\\components /y /s /i",
    "copy-content": "xcopy app\\content dist\\app\\content /y /s /i",
    "copy-deps": "xcopy deps dist\\deps /y /s /i",
    "copy-browser-source": "npm run copy-components & npm run copy-content & npm run copy-deps",
    "copy-ej-common": "xcopy \"..\\..\\assets\\scripts\\common\" node_modules\\syncfusion-javascript\\Scripts\\ej\\common /y /s /i",
    "copy-ej-web": "xcopy \"..\\..\\assets\\scripts\\web\" node_modules\\syncfusion-javascript\\Scripts\\ej\\web /y /s /i",
    "copy-ej-datavisualization": "xcopy \"..\\..\\assets\\scripts\\datavisualization\" node_modules\\syncfusion-javascript\\Scripts\\ej\\datavisualization /y /s /i",
    "copy-ej-i18n": "xcopy \"..\\..\\assets\\scripts\\i18n\" node_modules\\syncfusion-ej-global\\i18n /y /s /i",
    "copy-ej-src": "npm run copy-ej-common & npm run copy-ej-web & npm run copy-ej-datavisualization & npm run copy-ej-i18n",
    "copy-webconfig": "copy Web.config dist\\ /y & copy default.html dist\\ /y",
    "copy-ej-angular2-src": "xcopy \"..\\src\" node_modules\\ej-angular2\\src /y /s /i",
    "postinstall": "npm run copy-ejDef & npm run copy-ej-src & npm run lint & npm run copy-ej-angular2-src & npm run build & npm run copy-browser-source & npm run copy-webconfig"
  },  
  "dependencies": {
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@angular/router": "5.0.0",
    "@angular/upgrade": "5.0.0",
    "@types/ej.web.all": "0.0.2",
    "@types/jquery": "2.0.34",
    "@types/node": "6.0.52",
    "angular-in-memory-web-api": "0.1.1",
    "bootstrap": "^3.3.6",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.1",
    "jquery-validation": "^1.16.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.5.2",
    "ej-angular2": "github:syncfusion/ej-angular2#master",
    "syncfusion-javascript": "github:syncfusion/JavaScript-Widgets#master",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "angular2-router-loader": "0.3.5",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "^3.3.0",
    "bootstrap": "^3.3.6",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "~2.1.2",
    "file-loader": "~0.11.1",
    "html-loader": "~0.4.5",
    "html-webpack-plugin": "^2.16.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "~4.0.14",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.1",
    "typescript": "2.4.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.1",
    "url-loader": "^0.5.8",
    "webpack": "~2.7.0",
    "webpack-dev-server": "~2.4.5",
    "webpack-merge": "~4.1.0"
  }
}
